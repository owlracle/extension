{
    "name": "Owlracle",
    "description": "Multichain gas price tracker",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "img/icon.png",
            "32": "img/icon.png",
            "48": "img/icon.png",
            "128": "img/icon.png"
        }
    },
    "icons": {
        "16": "img/icon.png",
        "32": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "permissions": [
        "cookies"
    ],
    "update_URL": "https://edge.microsoft.com/extensionwebstorebase/v1/crx"
}